{
    "name": "template",
    "version": "1.0.0",
    "dependencies": {
        "tmodjs": "1.0.1-rc2"
    },
    "tmodjs-config": {
        "output": "../public/js",
        "charset": "utf-8",
        "syntax": "simple",
        "helpers": "./config/template-helper.js",
        "escape": true,
        "compress": true,
        "type": "default",
        "runtime": "template.js",
        "combo": true,
        "minify": true,
        "cache": true
    }
}